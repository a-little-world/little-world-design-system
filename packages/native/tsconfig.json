{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "outDir": "dist",
    "module": "ESNext",
    "target": "ES2017",
    "moduleResolution": "Node",
    "declaration": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
