    "debug:module:cjs": "echo '=== CJS Output ===\n' && echo '=== React Imports ===\n' && grep -n 'require.*react' node_modules/@a-little-world/little-world-design-system-native/dist/index.js || true && echo '\n=== React References ===\n' && grep -n 'React\\.' node_modules/@a-little-world/little-world-design-system-native/dist/index.js || true",
