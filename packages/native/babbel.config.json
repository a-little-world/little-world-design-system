{
    "presets": [
      ["@babel/preset-env", {
        "targets": {
          "node": "current"
        },
        "modules": false
      }],
      "@babel/preset-react",
      ["@babel/preset-typescript", {
        "isTSX": true,
        "allExtensions": true
      }]
    ],
    "plugins": [
      ["@babel/plugin-transform-runtime", {
        "useESModules": true
      }]
    ]
  }