{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["esnext", "DOM"],
    "module": "ES2015",
    "noImplicitAny": true,
    "outDir": "dist",
    "strict": true,
    "target": "ES5"
  },
  "include": ["src/*"],
  "exclude": ["node_modules", "dist"]
}
